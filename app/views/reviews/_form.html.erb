<%= simple_form_for [@baker, @review] do |f| %>
<%= f.error_notification %>

<div class="form-inputs">
  <%= f.input :rate, label: "Choose a rating", collection: [1,2,3,4,5] %>
  <%= f.input :content, wrapper_html: { rows: '5' } %>
</div>

<div class="form-actions">
  <%= f.button :submit %>
</div>
<% end %>
